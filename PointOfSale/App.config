<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IUserService" />
                <binding name="BasicHttpBinding_IMenuServcie" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/users" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IUserService" contract="UsersService.IUserService"
                name="BasicHttpBinding_IUserService" />
            <endpoint address="http://localhost:8080/menu" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMenuServcie" contract="MenuService.IMenuServcie"
                name="BasicHttpBinding_IMenuServcie" />
        </client>
    </system.serviceModel>
</configuration>